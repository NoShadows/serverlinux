<game_config>
  <game_key>cod_mw2_win32</game_key>
  <protocol>lgsl</protocol>
  <lgsl_query_name>callofdutymw2</lgsl_query_name>
  <game_name>CoD: Modern Warfare 2</game_name>
  <server_exec_name>iw4m.exe</server_exec_name>
  <cli_template>-dedicated 1 %MASTER% %EXEC% %GAMETYPE% %IP% %PLAYERS% %PORT% %FSGAME% +map_rotate</cli_template>
  <cli_params>
    <cli_param id="IP" cli_string="+set net_ip" options="s" />
    <cli_param id="PORT" cli_string="+set net_port" options="s" />
    <cli_param id="PLAYERS" cli_string="+set party_maxplayers" options="s" />
  </cli_params>
  <max_user_amount>18</max_user_amount>
  <control_protocol>rcon</control_protocol>
  <control_protocol_type>old</control_protocol_type>
  <mods>
    <mod key="default">
      <name>none</name>
    </mod>
  </mods>
  <server_params>
	<param id="GAMETYPE" key="+set gametype" type="text">
      <default>war</default>
      <caption>+set g_gametype</caption>
      <desc>"tdm" Team deathmatch|"war" War Mode|"dm" Free-for-all|"sab" Sabotage|"dem" Demolition|"ctf" Capture the flag|"sd" Search and destroy|"dom" Domination|"koth" Headquarters.</desc>
	</param>
    <param id="EXEC" key="+exec" type="text">
      <default>server.cfg</default>
      <caption>+exec</caption>
      <desc>Exec a server configuratipon file on startup.</desc>
    </param>
	<param id="MASTER" key="+sv_master1" type="text">
      <default>176.57.141.201</default>
      <caption>+sv_master1</caption>
      <desc>Master server IP address.</desc>
	</param>
	<param id="FSGAME" key="+set fs_game" type="text">
      <default></default>
      <caption>+set fs_game</caption>
      <desc>Set the mod path if any.</desc>
	</param>
  </server_params>
</game_config>