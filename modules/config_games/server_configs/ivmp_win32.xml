<game_config>
  <game_key>ivmp_win32</game_key>
  <protocol>lgsl</protocol>
  <lgsl_query_name>ivmp</lgsl_query_name>
  <game_name>GTA IV:MP</game_name>
  <server_exec_name>ivmp-svr.exe</server_exec_name>
  <max_user_amount>128</max_user_amount>
  <mods>
    <mod key="ivmp">
      <name>none</name>
    </mod>
  </mods>
  <replace_texts>
	<text key="port">
	  <default>port</default>
	  <var></var>
	  <filepath>settings.xml</filepath>
	  <options>tags</options>
	</text>
    <text key="home_name">
      <default>hostname</default>
      <var></var>
      <filepath>settings.xml</filepath>
      <options>tags</options>
    </text>
	<text key="max_players">
      <default>maxplayers</default>
      <var></var>
      <filepath>settings.xml</filepath>
      <options>tags</options>
    </text>
  </replace_texts>
 </game_config>
